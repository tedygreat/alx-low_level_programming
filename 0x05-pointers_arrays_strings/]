#include "main.h"

/**
 * rev_string - afunction that print string revers
 * @s: str input
 * Return: nothing
 */

void rev_string(char *s)
{

		int i, j;
			int strcoun = 0;
			int rev = s[0];
	for (i = 0; s[i] != '\0'; i++)
	{
		strcoun++;
	}

	for (j = strcoun - 1; j >= 0; j--)
	{
		rev = s[j];
	}
}
